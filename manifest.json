{
    "manifest_version": 2,
    "name": "Auto Connect LinkedIn",
    "description": "This extension use to auto connection in LinkedIn. If you feel it helpful, just support me.",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "https://www.linkedin.com/*"
            ],
            "js": [
                "foreground.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "storage"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Auto Connect LinkedIn"
    },
    "icons": {
        "16": "icons/icon16.png",
        "19": "icons/icon19.png",
        "38": "icons/icon38.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}